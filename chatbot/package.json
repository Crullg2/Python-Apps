{
  "name": "va-healthcare-chatbot",
  "version": "1.0.0",
  "description": "AI chatbot for VA healthcare questions, specifically focused on vasectomy and medical procedure information",
  "main": "chatbot.js",
  "scripts": {
    "start": "python -m http.server 8000",
    "dev": "python -m http.server 8080",
    "test": "node test/test-chatbot.js",
    "build": "npm run minify && npm run optimize",
    "minify": "terser chatbot.js -o chatbot.min.js --compress --mangle",
    "optimize": "html-minifier --input-dir . --output-dir dist --file-ext html"
  },
  "keywords": [
    "healthcare",
    "chatbot",
    "va",
    "medical",
    "vasectomy",
    "ai",
    "assistant",
    "powerbi"
  ],
  "author": "VA Healthcare Development Team",
  "license": "UNLICENSED",
  "engines": {
    "node": ">=14.0.0"
  },
  "devDependencies": {
    "terser": "^5.19.0",
    "html-minifier": "^4.0.0",
    "jest": "^29.5.0",
    "jsdom": "^22.1.0"
  },
  "repository": {
    "type": "git",
    "url": "file://./chatbot"
  },
  "bugs": {
    "url": "mailto:healthcare-dev@va.gov"
  },
  "homepage": "./index.html",
  "browserslist": [
    "last 2 versions",
    "not dead",
    "> 1%",
    "IE 11"
  ],
  "config": {
    "port": 8000,
    "host": "localhost"
  }
}
